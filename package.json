    "mcp-server.js",
    "cli.js",
    "index.js",
    "services",
    "README.md",
    "README_AI_MODELS.md",
    "LICENSE"
  ],
  "bin": {
    "icon-mcp": "cli.js",
    "icon-mcp-server": "mcp-server.js",
    "dt-query-icon": "mcp-server.js"
  },
  "scripts": {
    "start": "node index.js",
